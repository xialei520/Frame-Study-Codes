<!DOCTYPE html>
<html>
  <head>
    <title></title>
  </head>
  <body>
      list
      <table>
        <tr>
          <td>序号</td>
          <td>评论人</td>
          <td>标题</td>
          <td>内容</td>
         
        </tr>
        <% resData.forEach(function(item, index){ %>
          <tr>
            <td><%= index+1%></td>
            <td><%= item.username%></td>
            
            <td>
            <a href="/detail?cid=<%=parseInt(item.cid)%>">
              
              <%= item.title%>
            </a>
            </td>
            
            <td><%= item.val%></td>
          </tr>
        <% }) %>
        <tr colspan="1">
           <td>
            <a href="/list?pageNo=1">首页</a>
            <a href="/list?pageNo=<%= pageNo>1?parseInt(pageNo)-1:1 %>">上一页</a>
            第<%= pageNo%>页/共<%= totalpage%>页
            <a href="/list?pageNo=<%= pageNo < totalpage?parseInt(pageNo)+1:totalpage%>">下一页</a>
            <a href="/list?pageNo=<%= totalpage%>">最后一页</a>
            共<%=count%>条数据
           </td>
        </tr>
      </table>
  </body>
  
</html>